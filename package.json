{
  "name": "my-file-nyc-api",
  "version": "0.1.11",
  "description": "My File NYC Platform API",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc ./src/lambda-proxy.ts ./src/lambda-db-migrations.ts --outDir ./dist",
    "test": "jest",
    "format": "prettier --write \"{infra,src,test}/**/*.{js,ts}\"",
    "dev": "DEPLOYMENT_TARGET=dev . ./.env && npx ts-node ./src/local/main.ts",
    "generate-oas": "npm run build && ./node_modules/.bin/generate-oas -- ./dist/routes-config.js",
    "stack-destroy": ". ./params.env && npx cdk destroy --debug ",
    "stack-synth": ". ./params.env && npx cdk synth --debug ",
    "stack-diff": ". ./params.env && npx cdk diff --debug ",
    "stack-deploy": ". ./params.env && npx cdk deploy --debug --require-approval never",
    "schema-types:generate": "ts-node ./src/schema-to-interface.ts"
  },
  "author": "",
  "license": "ISC",
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "devDependencies": {
    "@aws-cdk/aws-apigatewayv2-alpha": "^2.81.0-alpha.0",
    "@aws-cdk/aws-apigatewayv2-integrations-alpha": "^2.81.0-alpha.0",
    "@types/aws-lambda": "^8.10.92",
    "@types/aws4": "^1.11.2",
    "@types/http-errors": "^2.0.4",
    "@types/jest": "^26.0.10",
    "@types/minimist": "^1.2.2",
    "@types/node": "^20.2.5",
    "@types/uuid": "^8.3.4",
    "aws-cdk": "^2.81.0",
    "aws-cdk-lib": "^2.81.0",
    "constructs": "^10.0.0",
    "esbuild": "^0.14.14",
    "express": "^4.18.2",
    "jest": "^26.4.2",
    "prettier": "^3.1.1",
    "prisma": "^5.6.0",
    "source-map-support": "^0.5.16",
    "ts-jest": "^26.2.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.267.0",
    "@aws-sdk/client-s3": "^3.485.0",
    "@aws-sdk/client-sqs": "^3.287.0",
    "@aws-sdk/s3-request-presigner": "^3.485.0",
    "@aws-sdk/util-dynamodb": "^3.267.0",
    "@myfile/core-sdk": "file:../na-my-file-core-sdk-pkg/myfile-core-sdk-0.1.0.tgz",
    "@prisma/client": "^5.7.0",
    "@prisma/migrate": "^5.8.1",
    "@types/express": "^4.17.21",
    "aws4": "^1.12.0",
    "axios": "^1.3.2",
    "date-fns-tz": "^2.0.0",
    "dotenv": "^16.3.1",
    "http-errors": "^2.0.0",
    "joi": "^17.11.0",
    "joi-to-typescript": "^4.12.0",
    "node-fetch": "^3.3.2",
    "qs": "^6.11.2"
  }
}
