{
  "name": "na-my-file-api",
  "version": "0.1.11",
  "description": "My File Platform API",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc ./src/lambda-proxy.ts --outDir ./dist",
    "test": "jest",
    "format": "prettier --write \"{infra,src,test}/**/*.{js,ts}\"",
    "dev": "DEPLOYMENT_TARGET=dev . ./.env && npx ts-node ./src/local/main.ts",
    "generate-oas-route-specs": "npm run build && ./node_modules/.bin/generate-oas -- ./dist/routes-config.js",
    "generate-oas-full": "./generate-oas-full-spec.js",
    "generate-oas": ". ./params.env && npm run generate-oas-route-specs && npm run generate-oas-full",
    "generate-api-client": "npx --yes @hey-api/openapi-ts@0.48.1 -i ./custom-swagger.json -o ../na-my-file-api-client/src",
    "ci-set-npmrc": ". ./params.env && ./build-npmrc.js",
    "stack-destroy": ". ./params.env && npx cdk destroy --debug ",
    "stack-synth": ". ./params.env && npx cdk synth --debug ",
    "stack-diff": ". ./params.env && npx cdk diff --debug ",
    "stack-deploy": ". ./params.env && npx cdk deploy -v --debug --require-approval never",
    "stack-deploy-dev": "DEPLOYMENT_TARGET=dev . ./params.env && npx cdk deploy -v --debug --require-approval never",
    "stack-deploy-prod": "DEPLOYMENT_TARGET=prod . ./params.env && npx cdk deploy -v --debug --require-approval never",
    "schema-types:generate": "ts-node ./src/schema-to-interface.ts",
    "db:setup": ". ./params.env && chmod +x ./src/scripts/db-setup-tunnel && ./src/scripts/db-setup-tunnel",
    "db:tunnel": ". ./params.env && chmod +x ./src/scripts/ssh-tunnel && ./src/scripts/ssh-tunnel"
  },
  "author": "",
  "license": "ISC",
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.92",
    "@types/aws4": "^1.11.2",
    "@types/http-errors": "^2.0.4",
    "@types/jest": "^26.0.10",
    "@types/joi": "^17.2.3",
    "@types/lodash": "^4.17.13",
    "@types/minimist": "^1.2.2",
    "@types/node": "^20.2.5",
    "@types/uuid": "^8.3.4",
    "aws-cdk": "^2.133.0",
    "aws-cdk-lib": "^2.133.0",
    "constructs": "^10.0.0",
    "esbuild": "~0.21.0",
    "express": "^4.18.2",
    "jest": "^26.4.2",
    "prettier": "^3.1.1",
    "prisma": "^5.6.0",
    "source-map-support": "^0.5.16",
    "ts-jest": "^26.2.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.267.0",
    "@aws-sdk/client-s3": "^3.485.0",
    "@aws-sdk/client-ses": "^3.699.0",
    "@aws-sdk/client-sqs": "^3.287.0",
    "@aws-sdk/s3-request-presigner": "^3.485.0",
    "@aws-sdk/util-dynamodb": "^3.267.0",
    "@prisma/client": "^5.7.0",
    "@types/express": "^4.17.21",
    "@types/mustache": "^4.2.5",
    "aws-lambda-api-tools": "^0.1.5",
    "aws4": "^1.12.0",
    "axios": "^1.3.2",
    "date-fns-tz": "^2.0.0",
    "dotenv": "^16.3.1",
    "gm": "^1.25.0",
    "http-errors": "^2.0.0",
    "https-proxy-agent": "^7.0.4",
    "image-size": "^1.1.1",
    "joi": "^17.12.3",
    "joi-to-typescript": "^4.12.0",
    "luxon": "^3.5.0",
    "mustache": "^4.2.0",
    "node-fetch": "^3.3.2",
    "pdf-lib": "^1.17.1",
    "qs": "^6.11.2"
  }
}
